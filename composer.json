{
    "name": "amendozaaguiar/filament-route-statistics",
    "description": "Filament route statictics viewer",
    "keywords": [
        "amendozaaguiar",
        "filament",
        "laravel",
        "route",
        "statistics"
    ],
    "require": {
        "php": "^8.1|^8.2",
        "filament/filament": "^3.0-stable",
        "bilfeldt/laravel-route-statistics": "^3.0|^4.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "filament/upgrade": "^3.0-stable"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Amendozaaguiar\\FilamentRouteStatistics\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anderson Mendoza Aguiar",
            "email": "amendozaaguiar@outlook.com",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amendozaaguiar\\FilamentRouteStatistics\\FilamentRouteStatisticsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
